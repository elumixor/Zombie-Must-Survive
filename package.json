    },
    "dependencies": {
        "@elumixor/di": "^0.2.1",
        "@elumixor/eslint": "^0.2.0",
        "@elumixor/frontils": "2.8.1",
        "@pixi/filter-color-overlay": "^5.1.1",
        "@pixi/layers": "^2.1.0",
        "@pixi/text-html": "^7.4.2",
        "gsap": "^3.12.5",
        "howler": "^2.2.4",
        "pixi-spine": "^4.0.4",
